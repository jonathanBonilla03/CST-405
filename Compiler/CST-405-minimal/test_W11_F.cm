    int num[5] = {1, 2, 3, 4, 5};
    int numLength = 5;
    int x = 0;

    while (x < numLength - 1) {
        if (num[x] < num[x+1]) {
            num = swap(num, x, x+1);
        }
        x = x + 1;
    }

    print(num);
    return 0;
