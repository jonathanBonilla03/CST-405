/*
 * Bubble Sort Algorithm in C-Minus
 * Demonstrates: arrays, nested loops, swapping
 */

void sort(int arr[], int n) {
    int i;
    int j;
    int temp;
    
    i = 0;
    while (i < n - 1) {
        j = 0;
        while (j < n - i - 1) {
            if (arr[j] > arr[j + 1]) {
                /* Swap elements */
                temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
            j = j + 1;
        }
        i = i + 1;
    }
}

void main(void) {
    int data[10];
    int size;
    int i;
    
    /* Input array size */
    size = input();
    
    /* Input array elements */
    i = 0;
    while (i < size) {
        data[i] = input();
        i = i + 1;
    }
    
    /* Sort the array */
    sort(data, size);
    
    /* Output sorted array */
    i = 0;
    while (i < size) {
        output(data[i]);
        i = i + 1;
    }
}