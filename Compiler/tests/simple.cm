/*
 * Simple test program for C-Minus compiler
 * Tests basic arithmetic and control flow
 */

void main(void) {
    int a;
    int b;
    int sum;
    int diff;
    int prod;
    int quot;
    
    /* Input two numbers */
    a = input();
    b = input();
    
    /* Arithmetic operations */
    sum = a + b;
    diff = a - b;
    prod = a * b;
    
    /* Division with zero check */
    if (b != 0) {
        quot = a / b;
    } else {
        quot = 0;
    }
    
    /* Output results */
    output(sum);
    output(diff);
    output(prod);
    output(quot);
}